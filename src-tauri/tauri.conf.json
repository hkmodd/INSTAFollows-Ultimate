{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "INSTAFollows Ultimate",
  "version": "1.0.0",
  "identifier": "com.sebas.instafollows-ultimate",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "INSTAFollows Ultimate",
        "width": 1280,
        "height": 800,
        "minWidth": 1024,
        "minHeight": 700,
        "center": true,
        "decorations": true,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}